mixin upload(item)
	.yi-upload&attributes(attributes)
		.upbox(class!= item.upBoxClass id!= item.upBoxId)
			if item.upFileIcon || item.upFileText
				.up-file
					i.icon(class!= item.upFileIcon)
					span.txt!= item.upFileText
			if item.upImgUrl
				if item.upImgBlock
					.up-img
						img.img(src!= item.upImgUrl)
						input.hide#Material
				else
					.up-img.hide
						img.img(src!= item.upImgUrl)
						input.hide#MaterialInfo_DealerMaterialBasicEntity_CoverageImgUrl(name='MaterialInfo.DealerMaterialBasicEntity.CoverageImgUrl', type='hidden', value='')
			
			if item.upNewIcon || item.upNewText
				if item.upNewBlock
					.up-new
						i.icon(class!= item.upNewIcon)
						span.txt!= item.upNewText
				else
					.up-new.hide
						i.icon(class!= item.upNewIcon)
						span.txt!= item.upNewText

		if item.upMsgText
			.up-msg!= item.upMsgText
		if block
			block