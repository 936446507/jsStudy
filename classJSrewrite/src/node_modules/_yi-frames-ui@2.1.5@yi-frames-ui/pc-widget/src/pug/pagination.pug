//- 翻页
mixin pagination(item)
	.yi-pagination&attributes(attributes)
		ul.pagewp(id!= item.ulID)
			li.disable
				a.lk(href='###') 首页
			li.prev.disable
				a.lk(href='###')
					i.icon.icon-previouspage
			li
				a.lk(href='###') 1
			li
				span.normal-state ...
			li
				a.lk(href='###') 67
			li.active
				a.lk(href='###') 67
			li
				a.lk(href='###') 69
			li
				a.lk(href='###') 70
			li
				a.lk(href='###') 71
			li
				span.normal-state ...
			li
				a.lk(href='###') 99
			li.next
				a.lk(href='###')
					i.icon.icon-nextpage
			li.next
				a.lk(href='###') 末页